{
  "name": "Next Date",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {},
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>",
    "declarativeContent"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["nextPage.js"]
    }
  ],
  "icons": {
    "16": "nextpage16.png",
    "32": "nextpage32.png",
    "48": "nextpage48.png",
    "128": "nextpage128.png"
  }
}
